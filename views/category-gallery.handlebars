<body class="d-flex h-100 text-bg-dark">
  <div class="d-flex" id="wrapper">
    <div class="border-end p-3" id="sidebar-wrapper">
      <div class="list-group list-group-flush" id="category-list-group">
        {{#each categories as |category|}}
        <a class="list-group-item list-group-item-action p-3 categories" href="/product/category/{{category.id}}"
          data-id={{category.id}}>{{category.category_name}}</a>
        {{/each}}
      </div>
    </div>  
      <!-- Page content wrapper-->
      <div id="page-content-wrapper">

      </div>
    </div>
  </div>
</div>
    <script src="/js/category.js"></script>
</body>

</html>